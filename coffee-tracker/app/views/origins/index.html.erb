<h1>List of Origins</h1><br>

<table>
	<tr>
		<th>Name</th>
		<th>Location</th>
		<th>Phone</th>
	</tr>

	<% @origins.each do |origin| %>
		<tr>
			<td><%= origin.name %></td>
			<td><%= origin.location %></td>
			<td><%= origin.phone %></td>
		</tr>
	<% end %>
</table><br>

<%= link_to 'Kopis', root_path %>