<h1>Listing all farms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Location</th>
    <th>Phone</th>
    <th></th>
  </tr>

  <% @farms.each do |farm| %>
    <tr>
      <td><%= farm.name %></td>
      <td><%= farm.location %></td>
      <td><%= farm.phone %></td>
    
    </tr>
  <% end %>
</table>