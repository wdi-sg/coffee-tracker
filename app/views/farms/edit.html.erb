<h1>Edit Farm</h1>

<%= form_with scope: :farm, model: @farm, local: true do |form| %>

	<p>
	  <%= form.label :origin %><br>
	  <%= form.text_field :origin %>
	</p>

	<p>
	  <%= form.label :location %><br>
	  <%= form.text_field :location %>
	</p>

	<p>
	  <%= form.label :phone %><br>
	  <%= form.text_field :phone %>
	</p>

	<p>
	<%= form.submit %>
	</p>

<% end %>

<%= link_to 'Back', farms_path, class: 'btn btn-success' %>