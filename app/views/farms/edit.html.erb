<h1>Edit Farm</h1>

<%= form_with scope: :farm, model: @farm, local: true do |form| %>


	<p>
		<%= form.label :location %><br>
		<%= form.text_area :location %>
	</p>

	<p>
		<%= form.label :phone_number %><br>
		<%= form.text_area :phone_number %>
	</p>

	<p>
		<%= form.submit %>
	</p>
<% end %>

<%= link_to 'Back', root_path %>