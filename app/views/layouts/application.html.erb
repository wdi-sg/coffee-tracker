<!DOCTYPE html>
<html>

<head>
		<title>KopiTracker</title>
		<%= csrf_meta_tags %>
		<%= csp_meta_tag %>
		<%= stylesheet_link_tag 'application', media: 'all' %>
		<%= javascript_pack_tag 'application' %>
</head>

<body>
		<%= render partial: "layouts/navbar", locals: {links: @links} %>
		<%= yield %>
		<p class="notice">
				<%= notice %>
		</p>
		<p class="alert">
				<%= alert %>
		</p>
</body>

</html>