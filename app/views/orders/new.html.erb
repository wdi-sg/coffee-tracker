
<%= form_with scope: :order, url: orders_path, local: true do |form| %>
  <p>
    <%= form.label :'Weight(pound)' %><br>
    <%= form.text_field :weight_in_pound %>
    
  </p>

  <p>
    <%= form.label :'Kopi' %><br>
    <%= form.collection_select :kopi_id, @kopis, :id, :name %>

  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>


<%= link_to 'Back', root_path %>