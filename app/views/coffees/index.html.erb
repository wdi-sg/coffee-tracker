<h1>Coffee Index</h1>

<h3> 
    <%= link_to 'New Coffeeeee', new_coffee_path %>
</h3>
<ul>
<% @coffees.each do |coffee| %>
    <li><%= coffee.name %><br>
    Price: $
    <%= coffee.price %><br>
    <%= coffee.origin.location %><br>
    <%= coffee.roast.name %><br>
    <%= link_to 'Show', coffee_path(coffee) %><br><br></li>
<% end %>
</ul>