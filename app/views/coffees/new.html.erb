<h1>Add a new type of Coffee!</h1>
<%= form_with scope: :coffee, url: coffees_path, local: true do |form| %>

  <p>
    <%= form.label :Name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :Roastedness %><br>
    <%= form.text_area :roastedness %>
  </p>

   <p>
    <%= form.label :Origin %><br>
    <%= form.text_area :origin %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>