<h1>Edit Kopi Bookmark for <%= current_user.email %></h1>

<%= form_with scope: :kopi_bookmark, url: kopi_bookmarks_path, model: @user, local: true do |form| %>
	<p>
		Kopi:
		<br>
		<%= form.collection_check_boxes :kopi_ids, @kopis, :id, :name %>
	</p>

  <p>
    <%= form.submit %>
  </p>
<% end %>

<br/>
<%= link_to 'Back', root_path, class: 'btn btn-primary' %>