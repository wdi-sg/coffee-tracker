<h1>Edit Origins Form</h1>
<%= form_with scope: :kopi, model: @kopi, local: true do |form| %>

  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :location %><br>
    <%= form.collection_select :origin_id, @origins, :id, :location %>
  </p>

  <p>
    <%= form.label :phone %><br>
    <%= form.collection_select :origin_id, @origins, :id, :phone %>
  </p>

      <p>
        <%= form.submit %>
      </p>
    <% end %>

<%= link_to 'Back', root_path %>
