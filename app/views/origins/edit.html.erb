<h1>Edit Origin</h1>

<%= form_with scope: :origin, model: @origin, local: true do |form| %>

  <p>
      <%= form.label :location %><br>
      <%= form.text_field :location %>
    </p>
  
    <p>
      <%= form.label :phone %><br>
      <%= form.text_area :phone %>
    </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', root_path %>