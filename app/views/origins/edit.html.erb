<h1>Edit Origins Form</h1>
<%= form_with scope: :origin, model: @origin, local: true do |form| %>

  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>

  <p>
    <%= form.label :location %><br>
    <%= form.text_field :location %>
  </p>

  <p>
    <%= form.label :phone %><br>
    <%= form.text_area :phone %>
  </p>

      <p>
        <%= form.submit %>
      </p>
    <% end %>

<%= link_to 'Back', origins_path %>

