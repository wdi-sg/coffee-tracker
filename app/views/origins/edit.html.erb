<h1>Edit origins</h1>
<%= form_with scope: :origin, model: @origin, local: true do |form| %>
 <p>
    <%= form.label :location %><br>
    <%= form.text_field :location%><br>
  </p>
  <p>
    <%= form.label :phone %><br>
    <%= form.text_field :phone%><br>
  </p>
<% end %>

<%= link_to 'Back', origins_path %>