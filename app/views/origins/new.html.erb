<h1>Add an Origin</h2>

<%= form_with scope: :origin, url: origins_path, local: true do |f| %>
    <p>
    <%= f.label :location %> <br/>
    <%= f.text_field :location %>
    </p>

    <p>
    <%= f.label :phone %><br/>
    <%= f.text_field :phone %>
    </p>

    <p>
    <%= f.submit %>
    </p>
<% end %>