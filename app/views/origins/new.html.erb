<%= form_with scope: :origin, url: origins_path, local: true do |form| %>

  <p>
    <%= form.label :location %><br>
    <%= form.text_field :location %>
  </p>

  <p>
    <label>Phone Number</label><br>
    <%= form.text_field :phone %>
  </p>


  <p>
    <%= form.submit %>
  </p>
<% end %>