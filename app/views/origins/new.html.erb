<h3>Add Origin</h3>

<%= form_with scope: :origin, url: origins_path, local: true do |form| %>

  <p>
    <%= form.label :location %><br>
    <%= form.text_area :location %>
  </p>

  <p>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', origins_path, class: "btn btn-dark" %>