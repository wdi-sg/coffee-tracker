<h1>Origin Add New Page</h1>

<%= form_with scope: :origin, url: origins_path, local: true do |form| %>

  <p class="error">
    <%= show_errors(@origin, :location) %>
    <%= show_errors(@origin, :phone) %>
  </p>

  <p>
    <%= form.label :location %><br>
    <%= form.text_field :location %>
  </p>

  <p>
    <%= form.label :phone %><br>
    <%= form.text_field :phone %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>