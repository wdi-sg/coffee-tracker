<h1>Origins</h1>



<% @origins.each do |origin| %>
<ul>
    <li><%= link_to origin.location, origin_path(origin) %></li>
</ul>
<% end %>

<% if user_signed_in? %>
    <%= link_to 'New Origin', new_origin_path %>
    <%= link_to 'log out', destroy_user_session_url, method: :delete %>
<% else%>
    <%= link_to 'log in', new_user_session_url %>
<% end %>