<p id="notice">
		<%= notice %>
</p>
<h1>Origins</h1>
<table>
		<thead>
				<tr>
						<th>Location</th>
						<th>Phone</th>
						<th colspan="3"></th>
				</tr>
		</thead>
		<tbody>
				<% @origins.each do |origin| %>
				<tr>
						<td>
								<%= origin.location %>
						</td>
						<td>
								<%= origin.phone %>
						</td>
						<% if @user %>
						<td>
								<%= link_to 'Show', origin %>
						</td>
						<td>
								<%= link_to 'Edit', edit_origin_path(origin) %>
						</td>
						<td>
								<%= link_to 'Destroy', origin, method: :delete, data: { confirm: 'Are you sure?' } %>
						</td>
						<% end %>
				</tr>
				<% end %>
		</tbody>
</table>
<br>
<%= link_to 'New Origin', new_origin_path if @user%>