<h1>List of origins</h1>
	<table>
		<% @origins.each do |origin| %>
		<tr>
			<th>Location</th>
			<th>Phone</th>
		</tr>
		<tr>
			<td><%= origin.location %></td>
			<td><%= origin.phone %></td>
		  <td><%= link_to 'Show', origin_path(origin)%></td>
			<td><%= link_to 'Delete', origin_path(origin), method: :delete%></td>
		</tr>
		<% end %>
	</table><br>
	<%= link_to 'Create Origin', new_origin_path %>
