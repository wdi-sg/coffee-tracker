<h1>Kopis#index</h1>
<p>Find me in app/views/kopis/index.html.erb</p>
<% @kopis.each do |kopi| %>
	<div>
		<%=kopi.name%>
	</div>

	<div>
		<%=kopi.roastedness%>
	</div>

	<div>
		<%=link_to kopi.farm.location, farm_path(kopi.farm)%>
	</div>

	<div>
		<%= link_to 'Show', kopi_path(kopi) %>
	</div>
<% end %>
