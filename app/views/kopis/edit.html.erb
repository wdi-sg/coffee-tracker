	<%= form_with model: @kopi, local: true do |f| %>
	<div class="form-group">
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</div>
	<div class="form-group">
		<%= f.collection_select :roast_id, Roast.all, :id, :name %>
	</div>
	<div class="form-group">
		<%= f.collection_select :origin_id, Origin.all, :id, :location %>
	</div>
	<%= link_to 'Back', kopis_path, class: "btn btn-primary" %>
	<%= f.submit class: "btn btn-primary" %>
	<% end %>
