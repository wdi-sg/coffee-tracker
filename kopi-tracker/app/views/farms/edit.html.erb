<h1> Edit Farm Page </h1>

<%= form_with scope: :farms, model: @farm, local: true do |form| %>
	<p>
    <%= form.label :location %><br>
    <%= form.text_field :location %>
  </p>

  <p>
    <%= form.label :phone %><br>
    <%= form.text_area :phone %>
  </p>

  <p>
    <%= form.submit %>
  </p>

<% end %>

<%= link_to 'Back', farms_path %>